{
  "name": "my-flux",
  "version": "0.0.1",
  "description": "My simple flux implementation for me.",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uhyo/my-flux.git"
  },
  "keywords": [
    "flux"
  ],
  "author": "uhyo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uhyo/my-flux/issues"
  },
  "homepage": "https://github.com/uhyo/my-flux#readme",
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.22",
    "@types/react-test-renderer": "^15.5.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-typescript": "^3.1.7",
    "jest": "^20.0.4",
    "merge2": "^1.0.3",
    "react": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "ts-jest": "^20.0.5",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "@types/react": "^15.0.25",
    "tslib": "^1.7.1"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/lib/**/*.ts{,x}"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts{,x}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/test/util/",
      "<rootDir>/dist/test/typing/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
